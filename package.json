{
  "name": "nonsmartcontracts",
  "scripts": {
    "build": "npm-run-all clean transpile",
    "dev": "NODE_ENV=development npm-run-all build serve",
    "run": "NODE_ENV=production npm-run-all build serve",
    "serve": "node ./src/app.js",
    "transpile": "babel ./src --out-dir dist",
    "clean": "rimraf dist",
    "watch:dev": "nodemon"
  },
  "nodemonConfig": {
    "exec": "npm run dev",
    "watch": [
      "src/*",
      "public/*"
    ],
    "ignore": [
      "**/__tests__/**",
      "*.test.js",
      "*.spec.js"
    ]
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  },
  "type": "module",
  "version": "1.0.0",
  "description": "simple contract templating tool",
  "main": "src/index.js",
  "repository": "git@github.com:DanielFortuyn/nonsmartcontracts.git",
  "author": "Daniel Fortuyn <daniel@fortuyn.nl>",
  "license": "GNU GPLv3",
  "private": false,
  "dependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.10.5",
    "@babel/preset-env": "^7.10.4",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "fuzzy": "^0.1.3",
    "handlebars": "^4.7.6",
    "inquirer": "^7.2.0",
    "inquirer-autocomplete-prompt": "^1.0.2",
    "marked": "^1.1.0",
    "md5": "^2.3.0",
    "moment": "^2.26.0",
    "node-telegram-bot-api": "^0.50.0",
    "npm-run-all": "^4.1.5",
    "pubsub-js": "^1.8.0",
    "rimraf": "^3.0.2",
    "smooch-core": "^8.10.2",
    "yaml": "^1.10.0"
  }
}
